module.exports = grammar({
  name: 'custom_lang',

  rules: {
    source_file: $ => repeat($.entry),

    entry: $ => seq(
      choice(
        $.s_entry,
        $.u_entry,
        $.a_entry,
        $.tc_entry,
        $.tr_entry,
        $.c_entry,
        $.error_entry
      ),
